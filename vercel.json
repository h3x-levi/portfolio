{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "github": {
    "enabled": true,
    "autoAlias": true,
    "autoExposeSystemEnvs": true
  },
  "env": [],
  "ignoreCommand": "git diff --quiet HEAD^ HEAD -- . ':(exclude)dist' ':(exclude).vercel' ':(exclude)vercel.json' ':(exclude)node_modules'",
  "installCommand": "npm install --legacy-peer-deps"
}